<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LK.Views.EventsPage">

    <StackLayout HorizontalOptions="Center" VerticalOptions="Center" BackgroundColor="White">
      <Label x:Name="messageLabel" FontSize="Medium" />
      <Button x:Name="editButton" Text="Edit Profile" Clicked="OnEditButtonClicked" />
      <Button x:Name="logoutButton" Text="Logout" Clicked="OnLogoutButtonClicked" />
    </StackLayout>

</ContentPage>